= form_for @test_entry do |f|
  - if @test_entry.errors.any?
    #error_explanation
      %h2= "#{pluralize(@test_entry.errors.count, "error")} prohibited this test_entry from being saved:"
      %ul
        - @test_entry.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :link
    = f.text_field :link
  .field
    = f.label :semester
    = f.text_field :semester
  .field
    = f.label :teacher
    = f.text_field :teacher
  .field
    = f.label :className_id
    = f.collection_select(:className_id , ClassName.order('name').all, :id ,:name)
  .field
    = f.label :classTestName_id
    = f.collection_select(:classTestName_id , ClassTestName.order('name').all, :id ,:name)
  .actions
    = f.submit 'Save'
